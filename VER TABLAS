USE GD1C2025; 
GO

PRINT '--- CONSULTA DE DATOS: DBO_SQUAD.Provincia ---';
SELECT TOP 1000 * FROM DBO_SQUAD.Provincia ORDER BY provincia_codigo;
SELECT COUNT(*) AS Total_Filas_Provincia FROM DBO_SQUAD.Provincia;
GO

