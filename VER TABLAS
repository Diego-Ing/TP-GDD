USE GD1C2025; 
GO

PRINT '--- CONSULTA DE DATOS: DB_SQUAD.Provincia ---';
SELECT TOP 1000 * FROM DB_SQUAD.Provincia ORDER BY provincia_codigo;
SELECT COUNT(*) AS Total_Filas_Provincia FROM DB_SQUAD.Provincia;
GO

